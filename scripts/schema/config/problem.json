{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Configuration File Schema -- \"Problem\"",
  "type": "object",
  "additionalProperties": false,
  "required": ["Type"],
  "properties":
  {
    "Type":
    {
      "type": "string",
      "enum": ["Eigenmode", "Driven", "Transient", "Electrostatic", "Magnetostatic", "ModeAnalysis", "QuadModeAnalysis"]
    },
    "Verbose": { "type": "integer", "minimum": 0 },
    "Output": { "type": "string" },
    "OutputFormats":
    {
      "type": "object",
      "additionalProperties": false,
      "properties":
      {
        "Paraview": { "type": "boolean" },
        "GridFunction": { "type": "boolean" }
      }
    }
  }
}
